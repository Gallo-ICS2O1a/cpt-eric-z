grid_layout = []
for blocks in range(22):
    grid_layout.append([0]*10)
    x = 150
    y = 50
print(grid_layout)
def setup():
    size(600,700)
def draw():
    global x
    global y
    background(255)
    rect(150,50,450,600)
    for grid_blocks in grid_layout:
        rect(x, y, 45 , 27.272727272727)
        x = x + 45
        y = y + 27.272727272
